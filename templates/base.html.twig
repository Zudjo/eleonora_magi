<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Eleonora Magi{% endblock %}</title>
        <link rel="icon" href="{{ asset('images/logo-nero-quadrato.webp') }}">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body id={% block body_id %}{% endblock %}>
        {{ include('components/header.html.twig') }}
        {% block body %}{% endblock %}
        {{ include('components/footer.html.twig') }}
    </body>
</html>
